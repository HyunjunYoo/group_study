############################
# 문제1. 거북이생성 및 사각형 그리기
import turtle

t = turtle.Turtle()
t.shape('turtle') 정

for i in range(4): 
    t.forward(200) 
    t.left(90) 
turtle.done()

############################
# 문제2. 오각별 그리기
import turtle

t = turtle.Turtle()
t.shape('turtle')

for i in range(5): 
    t.forward(200)
    t.left(144)
turtle.done() 
