<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Interval Challenge</title>
    <script>
        let number = 1; // number 변수를 1로 초기화
        let intervalId; // intervalId 변수 선언

        function printNumber() { // printNumber 함수 정의
        console.log(number); // 콘솔에 number 출력
        number++; // number 증가
            if (number > 10) { // number가 10 이상이면
                clearInterval(intervalId); // 간격을 지움
            }
        }
        intervalId = setInterval(printNumber, 2000); // 2초 간격으로 printNumber 함수를 호출하는 간격 설정
    </script>
</head>
<body>
    <!-- 이 예제에서는 본문이 비어 있음 -->
</body>
</html>
